#!/bin/sh

flink run -yt /etc/hadoop/conf/core-site.xml  -yt /etc/hbase/conf/hbase-site.xml  -yt /etc/hadoop/conf/hdfs-site.xml \
 --jobmanager yarn-cluster -yjm 3072 -ytm 3072  --detached --yarnname "Enrichments combined" \
 /opt/cloudera/CYBERSEC/jobs/flink-enrichment-combined-${cybersec.full.version}.jar /opt/cloudera/CYBERSEC/etc/conf/enrichment.properties

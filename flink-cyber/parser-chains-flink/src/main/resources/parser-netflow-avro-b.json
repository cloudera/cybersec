{
  "netflow_b": {
    "id": "220ee8c5-07d7-48d9-8df5-7d23376cb664",
    "name": "Netflow Parser",
    "parsers": [
      {
        "id": "f812c6dc-40cc-4c77-abf8-e15fccdfea32",
        "name": "Netflow as Avro",
        "type": "com.cloudera.parserchains.parsers.AvroParser",
        "config": {
          "input": {
            "input": "original_string"
          },
          "schemaPath": {
            "schemaPath": "avro-9009.schema"
          }
        }
      },
      {
        "id": "6b8797a2-95df-4021-83c2-60ac4c786e67",
        "name": "Field Renamer",
        "type": "com.cloudera.parserchains.parsers.RenameFieldParser",
        "config": {
          "fieldToRename": [
            {
              "from": "timestamp_start",
              "to": "timestamp"
            }
          ]
        }
      },
      {
        "id": "9549004f-83e4-4d24-8baa-abdbdad06e61",
        "name": "Timestamp Parser",
        "type": "com.cloudera.parserchains.parsers.TimestampFormatParser",
        "config": {
          "fields": [
            {
              "field": "timestamp",
              "format": "dd-MM-yyyy 'T' HH:mm:ss,yyyy-MM-dd'T'HH:mm:ss.SSS'Z',yyyy-MM-dd'T'HH:mm:ss.SS'Z',yyyy-MM-dd'T'HH:mm:ss.S'Z',yyyy-MM-dd'T'HH:mm:ss'Z'",
              "tz": "UTC"
            }
          ]
        }
      }
    ]
  }
}

{
  "isEnabled": "true",
  "type": "cybersec",
  "configs": {
    "username": "${cybersec_service_user_name}",
    "password": "${cybersec_service_user_name}",
    "policy.download.auth.users": "${cybersec_repo_users}",
    "tag.download.auth.users": "${cybersec_repo_users}",
    "default.policy.users": "${cybersec_repo_users}",
    "setup.additional.default.policies": "true",
    "default-policy.1.name": "default pipeline",
    "default-policy.1.resource.pipeline": "%default%",
    "default-policy.1.description": "Policy for default pipeline access",
    "default-policy.1.policyItem.1.accessTypes": "get,post,put,delete",
    "default-policy.1.policyItem.1.users": "${cybersec_repo_users}"
  },
  "name": "${SANITIZED_RANGER_SERVICE_NAME}",
  "description": "Cybersec service for ${CLUSTER_NAME}"
}
